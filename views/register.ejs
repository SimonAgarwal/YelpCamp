<%- include ("partials/header")%>
<link rel="stylesheet" href="stylesheets/register.css">
<div id="register" class="responsive">
<h1  style="text-align: center;width:40%;margin-bottom: 30px;">
	Sign Up
</h1>
<form action="/register" method="POST">
	<div class="form-group" >
	<input type="text" placeholder="username" name="username" style="width:40%;border-radius:5px;border:1px solid black;height: 40px;">
	</div>
	<div class="form-group" >
	<input type="password" placeholder="password" name="password" style="width:40%;border-radius:5px;border:1px solid black;height: 40px;">
	</div>
	<button  class="btn btn-primary" style="width:40%;border-radius: 5px;">
		SignUp
	</button>
	
</form>
</div>
<%- include ("partials/footer")%>