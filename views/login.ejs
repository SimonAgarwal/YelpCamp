<%- include ("partials/header")%>

<link rel="stylesheet" href="stylesheets/login.css">
<div  id="login" class="responsive">
<h1 style="text-align: center;width:40%;margin-bottom: 30px;">
	LOG IN
</h1>
<div >
<form action="/login" method="POST" >
	<div class="form-group" >
	<input type="text" placeholder="username" name="username" style="width:40%;border-radius:5px;border:1px solid black;height: 40px;">
	</div>
	<div class="form-group">
	<input type="password" placeholder="password" name="password" style="width:40%;border-radius: 5px;border-radius:5px;border:1px solid black;height: 40px;">
	</div>

	<input type="submit" value="login" class="btn btn-primary" style="width:40%;border-radius: 5px;">
	
</form>
</div>
</div>

<%- include ("partials/footer")%>